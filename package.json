      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "husky": "^7.0.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23"
